<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="700.0" prefWidth="800.0"
      xmlns="http://javafx.com/javafx/8.0.202" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController">
   <children>
      <HBox alignment="CENTER" prefHeight="55.0" prefWidth="800.0">
         <children>
            <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Choose image for apply static filters">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Text>
            <TextField fx:id="textFieldFile" disable="true" prefHeight="26.0" prefWidth="287.0">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </TextField>
            <Button fx:id="buttonOpen" mnemonicParsing="false" onAction="#onClickOpenButton" text="Open">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <VBox.margin>
            <Insets />
         </VBox.margin>
      </HBox>
      <Text fx:id="textOriginallyImage" strokeType="OUTSIDE" strokeWidth="0.0" text="Originally image">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></Text>
      <ImageView fx:id="originallyImage" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
         <VBox.margin>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </VBox.margin></ImageView>
      <HBox alignment="CENTER" prefHeight="43.0" prefWidth="800.0">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <children>
            <Text fx:id="textApplyFilters" strokeType="OUTSIDE" strokeWidth="0.0" text="Apply filters">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Text>
            <Button fx:id="buttonOk" mnemonicParsing="false" onAction="#onClickOkButton" text="Ok">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
      <HBox alignment="TOP_CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Text fx:id="textFilter1" strokeType="OUTSIDE" strokeWidth="0.0" text="Filter Min">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></Text>
                  <ImageView fx:id="filter1" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></ImageView>
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Text fx:id="textFilter2" strokeType="OUTSIDE" strokeWidth="0.0" text="Filter Mean">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></Text>
                  <ImageView fx:id="filter2" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></ImageView>
               </children>
            </VBox>
            <VBox prefHeight="200.0" prefWidth="100.0">
               <children>
                  <Text fx:id="textFilter3" strokeType="OUTSIDE" strokeWidth="0.0" text="Filter Max">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></Text>
                  <ImageView fx:id="filter3" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <VBox.margin>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                     </VBox.margin></ImageView>
               </children>
            </VBox>
         </children>
      </HBox>
      <Button fx:id="buttonSave" mnemonicParsing="false" onAction="#onClickSaveButton" text="Save images" />
      <Text fx:id="textInfo" strokeType="OUTSIDE" strokeWidth="0.0" />
      <ProgressBar fx:id="progressBar" prefWidth="200.0" progress="0.0" visible="false" />
   </children>
</VBox>
